<p id="notice"><%= notice %></p>

<h1>Whattimes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whattimes.each do |whattime| %>
      <tr>
        <td><%= whattime.name %></td>
        <td><%= link_to 'Show', whattime %></td>
        <td><%= link_to 'Edit', edit_whattime_path(whattime) %></td>
        <td><%= link_to 'Destroy', whattime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Whattime', new_whattime_path %>
